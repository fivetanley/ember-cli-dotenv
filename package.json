{
  "name": "ember-cli-dotenv",
  "version": "2.2.0",
  "description": "Expose .env variables in Ember applications",
  "keywords": [
    "ember-addon",
    "dotenv"
  ],
  "repository": "https://github.com/fivetanley/ember-cli-dotenv",
  "license": "MIT",
  "author": "Stanley Stuart",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/fivetanley/ember-cli-dotenv/issues"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "npm run test:ember && npm run test:node",
    "test:node": "mocha",
    "test:ember": "ember try:each"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "ember-cli-babel": "^6.6.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.7.0",
    "chai": "^4.1.2",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.3.0",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.3.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "ember-welcome-page": "^3.0.0",
    "fs-extra": "^7.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.2.3",
    "mocha": "^4.0.1",
    "qunit-dom": "^0.6.2"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
