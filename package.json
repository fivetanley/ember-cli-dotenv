{
  "name": "@precision-nutrition/ember-cli-dotenv",
  "version": "2.3.1",
  "description": "Expose .env variables in Ember applications",
  "keywords": [
    "ember-addon",
    "dotenv"
  ],
  "repository": "https://github.com/PrecisionNutrition/ember-cli-dotenv",
  "license": "MIT",
  "author": "Stanley Stuart",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bugs": {
