    "eslint-plugin-node": "^10.0.0",
    "fs-extra": "^7.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^4.0.1",
    "qunit-dom": "^0.9.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
