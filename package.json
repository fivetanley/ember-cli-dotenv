{
  "name": "ember-cli-dotenv",
  "version": "3.1.0",
  "description": "Expose .env variables in Ember applications",
  "keywords": [
    "ember-addon",
    "dotenv"
  ],
  "repository": "https://github.com/fivetanley/ember-cli-dotenv",
  "license": "MIT",
  "author": "Stanley Stuart",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/fivetanley/ember-cli-dotenv/issues"
  },
  "scripts": {
    "build": "ember build",
    "lint": "yarn lint:js && yarn lint:hbs",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "npm run test:ember && npm run test:node",
    "test:node": "mocha",
    "test:ember": "ember try:each"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "ember-cli-babel": "^7.23.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@ember/optional-features": "^2.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "ember-cli": "^3.20.0",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.0.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^4.0.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.20.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "ember-welcome-page": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-plugin-ember": "^9.6.0",
    "eslint-plugin-node": "^11.1.0",
    "fs-extra": "^9.0.1",
    "loader.js": "^4.7.0",
    "mocha": "^8.2.1",
    "qunit-dom": "^1.6.0"
  },
  "engines": {
    "node": ">=12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
